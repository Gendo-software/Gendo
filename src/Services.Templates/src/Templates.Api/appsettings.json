{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "mongo": {
    "connectionString": "mongodb://mongo:27017",
    "database": "templates-service"
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": true,
    "influxUrl": "http://influxdb:8086",
    "database": "templates",
    "env": "local",
    "interval": 5
  },
  "jwt": {
    "issuer": "https://dev-gendo.eu.auth0.com/",
    "audience": "basicApi"
  },
  "AllowedHosts": "*",
  "rabbitMq": {
    "namespace": "gendoc",
    "username": "rabbitmq",
    "password": "rabbitmq",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "rabbitmq"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  }
}
