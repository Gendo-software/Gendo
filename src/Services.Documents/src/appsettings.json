{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "mongo": {
    "connectionString": "mongodb://mongo:27017",
    "database": "documents-service"
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": true,
    "influxUrl": "http://influxdb:8086",
    "database": "Documents",
    "env": "local",
    "interval": 5
  },
  "jwt": {
    "issuer": "https://dev-gendo.eu.auth0.com/",
    "audience": "basicApi"
  },
  "AllowedHosts": "*"
}
